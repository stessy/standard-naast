<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<TitledPane alignment="CENTER" collapsible="false" text="Prix des abonnements" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="standardNaast.view.season.SeasonAbonnementPricesController">
   <content>
      <BorderPane>
         <center>
            <TableView fx:id="abonnementPricesTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="blocColumn" prefWidth="75.0" text="Bloc" />
                <TableColumn fx:id="lessThanTwelveColumn" prefWidth="75.0" text="-12" />
                  <TableColumn fx:id="twelveEighteenColumn" prefWidth="75.0" text="12-18" />
                  <TableColumn fx:id="studentColumn" prefWidth="75.0" text="Etudiant" />
                  <TableColumn fx:id="seniorColumn" prefWidth="75.0" text="Senior" />
                  <TableColumn fx:id="fullPriceColumn" prefWidth="75.0" text="Prix plein" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
         <bottom>
            <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <VBox alignment="TOP_CENTER" spacing="5.0">
               <children>
                  <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="NEVER">
                     <children>
                        <Label alignment="TOP_LEFT" text="Type compÃ©tition" />
                        <ComboBox fx:id="competitions" onAction="#onSelectedCompetitionType" prefWidth="150.0" />
                        <Button disable="true" mnemonicParsing="false" onAction="#onOpen" text="Importer les prix des abonnements" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </top>
      </BorderPane>
   </content>
</TitledPane>
